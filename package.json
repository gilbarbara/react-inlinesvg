{
  "name": "react-inlinesvg",
  "version": "0.5.1",
  "description": "An SVG loader for React",
  "repository": {
    "type": "git",
    "url": "git://github.com/matthewwithanm/react-inlinesvg.git"
  },
  "keywords": [
    "react-component",
    "react",
    "svg",
    "component"
  ],
  "author": "Matthew Dapena-Tretter <m@tthewwithanm.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthewwithanm/react-inlinesvg/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "httpplease": "^0.16",
    "once": "^1.3",
    "fbjs": "^0.8"
  },
  "devDependencies": {
    "babel-core": "^6.9",
    "babel-eslint": "^6.0",
    "babel-plugin-add-module-exports": "^0.2",
    "babel-preset-es2015": "^6.9",
    "babel-preset-react": "^6.5",
    "babel-preset-stage-1": "^6.5",
    "babel-register": "^6.9",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "cors": "^2.7",
    "eslint": "^2.10",
    "eslint-config-airbnb": "^9.0",
    "eslint-plugin-import": "^1.7",
    "eslint-plugin-jsx-a11y": "^1.2",
    "eslint-plugin-react": "^5.1",
    "expect": "^1.20",
    "gulp": "^3.9",
    "gulp-babel": "^6.1",
    "gulp-bump": "^2.1.0",
    "gulp-connect": "^4.0",
    "gulp-load-plugins": "^1.2",
    "gulp-rename": "^1.2",
    "gulp-util": "^3.0",
    "jsdom": "^9.1",
    "mocha": "^2.4",
    "babel-plugin-rewire": "1.0.0-rc-3",
    "react": "^15.1",
    "react-addons-test-utils": "^15.1",
    "react-dom": "^15.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0"
  },
  "scripts": {
    "build": "gulp build",
    "test": "gulp test-server & mocha --compilers js:babel-register; kill %1",
    "lint": "eslint -c ./.eslintrc $(find src -name '*.js')",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
	"plugins": [
	  "add-module-exports"
	]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "/bower_components/",
          "sourceMapRelative": "."
        }
      ]
    ]
  }
}
