{
  "presets": [
    [
      "env", {
      "useBuiltIns": true,
      "modules": false,
      "targets": {
        "node": "8"
      }
    }
    ],
    "react",
    "stage-1"
  ],
  "env": {
    "production": {
      "plugins": [
        "array-includes",
        "transform-flow-strip-types"
      ]
    },
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ],
      "sourceMaps": "both"
    }
  }
}
